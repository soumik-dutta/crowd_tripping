<script language="javascript">
    $( document ).ready(function() {
         //check for lat and lng in the cookies 
        //if found set the graph in that location
        //else set to default
        var lat=getCookie('latitude');
        var lng=getCookie('longitude');
        //var map={};
        if(lat=="" || lng==""){
            //alert('cookie not set')
            var map=new GMaps({
              div: '#map',
              lat: -12.043333,
              lng: -77.028333
            });
            //geoloaction
             GMaps.geolocate({
            success: function(position){
              map.setCenter(position.coords.latitude, position.coords.longitude);
                setCookie('latitude',position.coords.latitude);
                setCookie('longitude',position.coords.longitude);
                //marker
                map.addMarker({
                  lat: position.coords.latitude,
                  lng: position.coords.longitude,
                  title: 'Lima',

                });
            },
            error: function(error){
              alert('Geolocation failed: '+error.message);
            },
            not_supported: function(){
              alert("Your browser does not support geolocation");
            },
            always: function(){
             // alert("Done!");
            }      
          });
        }
        else{
            
           var map = new GMaps({
                      div: '#map',
                      lat: lat,
                      lng: lng
            });
            map.addMarker({
                  lat: lat,
                  lng: lng,
                  title: 'Lima',

                });
        }
        
        
        
        
        
        
    });
    
    
    //$.get() to get places 
    $.getJSON("/getPlaces", function (data) {
        //iteration within the data
        $.each(data, function (i, data_array) {
               var div_data = "<option value=" + data_array.place + ">" + data_array.place + "</option>";
                //alert(div_data);
                $(div_data).appendTo('#addPlaces');
            //alert(data_array.place)
        });
    });
    
       
    
   $('#search').click(function(){
            alert($('#addPlaces').val().trim())
        var address=$('#addPlaces').val().trim();
       GMaps.geocode({
          address:'kolkata',
          callback: function(results, status){
              alert(results[0].geometry.location)
            if(status=='OK'){
              var latlng = results[0].geometry.location;
              map.setCenter(latlng.lat(), latlng.lng());
              map.addMarker({
                lat: latlng.lat(),
                lng: latlng.lng()
              });
            }
          }
        });
       
      
   });
    
     
      
    
    
